{
    "options": [
        {
            "short": "-h",
            "long": "--help",
            "description": "Show help message",
            "action": "sh $ENHANCD_ROOT/custom/options/help.sh"
        },
        {
            "short": "-V",
            "long": "--version",
            "description": "Show version information",
            "action": "sh $ENHANCD_ROOT/custom/options/version.sh"
        },
        {
            "short": "-c",
            "long": "--current",
            "description": "Filter current directories that have been to",
            "action": "__enhancd::custom::current ${1:+\"$@\"}"
        },
        {
            "short": "-g",
            "long": "--ghq",
            "description": "Filter ghq list and cd to it",
            "action": "__enhancd::custom::ghq ${1:+\"$@\"}"
        }
    ]
}
